<apex:page standardController="Case" lightningStylesheets="true" extensions="CONS_visualizarcumbre" cache="true" showHeader="false" sidebar="true" >
<!--    
	<apex:iframe src="{!url2}" scrolling="true" id="theIframe22"  width="100%" height="900px" />
	<apex:pageMessages rendered="{!error}"/>  
-->
    <apex:includeScript value="https://ajax.googleapis.com/ajax/libs/jquery/1.7.2/jquery.min.js" />
    <apex:includeScript value="https://ajax.googleapis.com/ajax/libs/jqueryui/1.11.4/jquery-ui.min.js" />
    <apex:includeScript value="https://ajax.googleapis.com/ajax/libs/jquery/1.7.2/jquery.min.js" />
    <apex:includeScript value="/soap/ajax/36.0/connection.js"/>
    <apex:includeScript value="/soap/ajax/36.0/apex.js"/>  
    <apex:includeScript value="/support/console/44.0/integration.js"/>
    <script type="text/javascript">
    
    urllink();
    init();
    function urllink() {
        var url="{!url2}";
        window.open(url, '_blank');
    }
    
    function init()
    {   
        setTimeout(function() {                
            try {
                var btnJsActivado = document.getElementById('pagina2:from2:pgBlk:pgbloks:pgBlkSecItem:cmbnn');
                btnJsActivado.click();
            } catch(e) {
            }
        }, 1000);
    }  
    </script>
    <script type="text/javascript">
    //Close tab in Console    
    var callCloseTab = function callCloseTab(result) {
        sforce.console.closeTab(result.id);
    }        
    function closeTab() {
        sforce.console.getEnclosingTabId(callCloseTab);
    }
    
    </script>  
    <apex:pageBlock id="pagina2" rendered="true">
    </apex:pageBlock>
    <apex:form id="from2"  style="display:none" >
        <apex:pageMessages rendered="{!error}"/>
        <apex:pageBlock id="pgBlk">
            <apex:pageBlockSection id="pgbloks">
                <apex:pageBlockSectionItem id="pgBlkSecItem">
                    <apex:commandButton id="cmbnn" onclick="closeTab();return false" value="volver"/>                            
                </apex:pageBlockSectionItem>
            </apex:pageBlockSection>                
        </apex:pageBlock>
    </apex:form>
</apex:page>